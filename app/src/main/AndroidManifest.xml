<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.velsol.animallife">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/animfruit"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/animfruit"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AnimalActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".ImageShow"
            android:screenOrientation="portrait"/>
        <activity android:name=".BirdsActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".FruitsZoomAct"
            android:screenOrientation="portrait"/>
    </application>

</manifest>